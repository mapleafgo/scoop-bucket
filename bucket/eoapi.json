{
    "version": "1.7.0",
    "homepage": "https://www.eoapi.io/",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/eolinker/eoapi/releases/download/v$version/eoapi-Setup-$version.exe#/dl.7z"
    },
    "architecture": {
        "64bit": {
            "installer": {
                "script": "7z x $original_dir/PLUGINSDIR/app-64.7z -o\"$original_dir\""
            }
        }
    },
    "extract_dir": "$PLUGINSDIR",
    "extract_to": "PLUGINSDIR",
    "post_install": "rm $original_dir/PLUGINSDIR -recurse",
    "shortcuts": [
        [
            "eoapi.exe",
            "Eoapi"
        ]
    ]
}
